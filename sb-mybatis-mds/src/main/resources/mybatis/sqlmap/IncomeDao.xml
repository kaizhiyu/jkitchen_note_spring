<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sauzny.sb_mybatis_mds.dao.IncomeDao">

 <select id="findAll" resultMap="com.sauzny.sb_mybatis_mds.dao.IncomeMapper.BaseResultMap">
     select
     <include refid="com.sauzny.sb_mybatis_mds.dao.IncomeMapper.Base_Column_List" />
     from income
 </select>

 <select id="findByPage" resultMap="com.sauzny.sb_mybatis_mds.dao.IncomeMapper.BaseResultMap">
     select
     <include refid="com.sauzny.sb_mybatis_mds.dao.IncomeMapper.Base_Column_List" />
     from income
 </select>

 <select id="findByExamplePage" parameterType="com.sauzny.sb_mybatis_mds.entity.pojo.IncomeExample"
     resultMap="com.sauzny.sb_mybatis_mds.dao.IncomeMapper.BaseResultMap">
     select
     <if test="distinct">
         distinct
     </if>
     <include refid="com.sauzny.sb_mybatis_mds.dao.IncomeMapper.Base_Column_List" />
     from income
     <if test="_parameter != null">
         <include refid="com.sauzny.sb_mybatis_mds.dao.IncomeMapper.Example_Where_Clause" />
     </if>
     <if test="orderByClause != null">
         order by ${orderByClause}
     </if>
 </select>

</mapper>
